    </div>
    ` %>
    <% rolName = "Colaborador"; %>
<% } else if (admin == 1) { %>
    <% isAdmin = `<h1 class="text-danger">Admin Dashboard</h1>`;%>
    <% navContent += `
        <div class="menu mt-5">
            <a href="${finalURL}/dashboard" class="d-block am text-light p-4 lead text-decoration-none"><ion-icon name="home-outline" class="me-2"></ion-icon>Menu Principal</a>
            <a href="${finalURL}/dashboard/add-users" class="d-block am text-light p-4 lead text-decoration-none"><ion-icon name="person-add-outline" class="me-2"></ion-icon>Ingresar Usuario</a>
